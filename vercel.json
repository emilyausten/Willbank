{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.htm"
    },
    {
      "src": "/chat/pagamento",
      "dest": "/chat/pagamento/index.html"
    },
    {
      "src": "/chat/checkout-transparente",
      "dest": "/chat/checkout-transparente/index.html"
    },
    {
      "src": "/chat/sucesso",
      "dest": "/chat/sucesso/index.html"
    },
    {
      "src": "/chat/simulacao",
      "dest": "/chat/simulacao/index.htm"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 